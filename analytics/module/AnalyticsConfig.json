{
  "BQS": {
    "hostname": "bqs",
    "port": 8086,
    "proxy": false
  },
  "MYSQL": {
    "hostname": "db"
  },
  "periodic_jobs": {
    "max_run_time_in_s": 432000,
    "period_in_s": 180,
    "high_freq_db_period": 30,
    "low_freq_db_period": 900
  },
  "pipelines": {
    "link_mean_variance_pipeline": {
      "command": "python3 {}/link_insights/link_pipeline.py link_mean_variance_pipeline",
      "metric_names": [
        "phystatus.ssnrest",
        "stapkt.txpowerindex",
        "stapkt.mcs"
      ],
      "period_in_mins": 2,
      "sample_duration_in_s": 3600,
      "source_db_interval": 30
    },
    "traffic_stats_pipeline": {
      "command": "python3 {}/link_insights/link_pipeline.py traffic_stats_pipeline",
      "period_in_mins": 2,
      "sample_duration_in_s": 3600,
      "source_db_interval": 30
    },
    "link_health_pipeline": {
      "command": "python3 {}/link_insights/link_pipeline.py link_health_pipeline",
      "period_in_mins": 2,
      "sample_duration_in_s": 3600,
      "source_db_interval": 30,
      "green_cutoff_ratio": 0.95,
      "amber_cutoff_ratio": 0.75
    },
    "link_foliage_pipeline": {
      "command": "python3 {}/link_insights/link_pipeline.py link_foliage_pipeline",
      "period_in_mins": 2,
      "sample_duration_in_s": 3600,
      "source_db_interval": 1,
      "foliage_threshold": 0.85,
      "number_of_windows": 5,
      "min_window_size": 20,
      "minimum_var": 0
    }
    ,
    "link_interference_pipeline": {
      "command": "python3 {}/link_insights/link_pipeline.py link_interference_pipeline",
      "period_in_mins": 2,
      "sample_duration_in_s": 600,
      "source_db_interval": 30
    }
  }
}
