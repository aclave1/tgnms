{
  "BQS": {
    "hostname": "bqs",
    "port": 8086,
    "proxy": false
  },
  "MYSQL": {
    "hostname": "db"
  },
  "periodic_jobs": {
    "max_run_time_in_s": 432000,
    "period_in_s": 180
  },
  "pipelines": {
    "link_mean_variance_pipeline": {
      "command": "python3 {}/link_insights/link_pipeline.py link_mean_variance_pipeline",
      "metric_names": [
        "phystatus.ssnrest",
        "stapkt.txpowerindex",
        "stapkt.mcs"
      ],
      "period_in_mins": 2,
      "sample_duration_in_s": 3600,
      "source_db_interval": 30
    },
    "traffic_stats_pipeline": {
      "command": "python3 {}/link_insights/link_pipeline.py traffic_stats_pipeline",
      "period_in_mins": 2,
      "sample_duration_in_s": 3600,
      "source_db_interval": 30
    }
  }
}
