{
  "num_consumers": 5,
  "pipelines": {
    "pipeline 1": {
      "period": 86400,
      "jobs": [
        {
          "name": "cut_edge_finder",
          "window_s": 86400,
          "enabled": true,
          "params": {
            "config_change_delay_s": 60,
            "link_flap_backoff_ms": "1050",
            "link_impairment_detection": 0,
            "link_uptime_threshold": 0.5
          }
        },
        {
          "name": "configure_tideals",
          "enabled": true,
          "params": {
            "wireless_capacity_mbps": 1000,
            "wired_capacity_mbps": 100000
          }
        }
      ]
    }
  }
}
