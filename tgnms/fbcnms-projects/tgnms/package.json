{
  "name": "@fbcnms/tg-nms",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "yarn run start:dev",
    "start:dev": "nodemon -w scripts/ -w server/ scripts/server --exec 'node -r @fbcnms/babel-register --inspect'",
    "build": "rimraf dist && NODE_ENV=production node -r '@fbcnms/babel-register' ../../node_modules/webpack/bin/webpack --config ./config/webpack.production.config.js --progress --profile --colors",
    "eslint": "./node_modules/.bin/eslint --ignore-path .eslintignore",
    "sequelize": "./node_modules/.bin/sequelize --seeders-path server/seeders --migrationss-path server/migrations $*",
    "gen-migration": "./node_modules/.bin/sequelize migration:generate --migrations-path server/migrations/",
    "jscs": "jscs .",
    "precommit": "./node_modules/.bin/lint-staged",
    "thrift-to-flow": "thrift2flow --path $(pwd)/shared/types/ thrift/*.thrift && mv thrift/*.js shared/types/",
    "test": "(cd ../../ && yarn run test)",
    "test:app": "(cd ../../ && yarn run test app)",
    "test:server": "(cd ../../ && yarn run test server)"
  },
  "dependencies": {
    "@fbcnms/babel-register": "^0.1.0",
    "@fbcnms/i18n": "^0.1.0",
    "@material-ui/core": "^4.0.0",
    "@material-ui/icons": "^4.0.0",
    "@material-ui/styles": "^4.0.0",
    "@testing-library/react": "^8.0.2",
    "@testing-library/react-hooks": "^1.0.4",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.5",
    "clipboard": "^2.0.1",
    "compression": "^1.7.1",
    "connect-session-sequelize": "^6.0.0",
    "cookie-parser": "^1.4.3",
    "copy-to-clipboard": "^3.0.8",
    "css-loader": "^1.0.1",
    "d3-scale": "^2.1.2",
    "diff": "^3.4.0",
    "dom-helpers": "^3.3.1",
    "dotenv": "^6.0.0",
    "equals": "^1.0.5",
    "eventsource": "^1.0.7",
    "express-session": "^1.15.6",
    "express-ws": "^2.0.0",
    "geolib": "^2.0.24",
    "i18next": "^15.0.9",
    "i18next-xhr-backend": "^2.0.1",
    "is-ip": "^2.0.0",
    "kafka-node": "^4.1.3",
    "lodash": "^4.17.11",
    "mapbox-gl": "^0.53.0",
    "moment": "^2.24.0",
    "multer": "^1.3.0",
    "mysql2": "^1.5.3",
    "openid-client": "2.4.5",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-strategy": "^1.0.0",
    "plotly.js-basic-dist": "^1.42.5",
    "pug": "^2.0.3",
    "rc-slider": "^8.6.7",
    "react": "^16.8.1",
    "react-debounce-input": "^3.2.0",
    "react-dom": "^16.8.1",
    "react-hot-loader": "^4.8.0",
    "react-i18next": "^10.9.0",
    "react-jss": "^8.6.1",
    "react-mapbox-gl": "^4.0.2",
    "react-plotly.js": "^2.2.0",
    "react-router-dom": "^4.4.0-beta.6",
    "react-select": "^2.1.2",
    "react-virtualized": "^9.21.0",
    "request": "^2.79.0",
    "semver": "^5.6.0",
    "sequelize": "^5.8.5",
    "sequelize-cli": "^5.4.0",
    "style-loader": "^0.23.1",
    "tail": "^2.0.2",
    "thrift": "^0.11.0",
    "umzug": "^2.1.0",
    "uuid": "^3.3.2",
    "webpack-parallel-uglify-plugin": "^1.1.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^8.0.2",
    "autoprefixer": "7.1.6",
    "babel-loader": "^8.0.5",
    "express": "^4.16.3",
    "file-loader": "2.0.0",
    "jest": "24.8.0",
    "jest-dom": "^3.1.3",
    "lint-staged": "^7.1.3",
    "nodemon": "^1.18.10",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "3.0.0",
    "prop-types": "^15.5.8",
    "react-dev-utils": "^5.0.1",
    "supertest": "^4.0.2",
    "sweetalert2": "7.28.4",
    "thrift2flow": "^0.6.0",
    "url-loader": "1.1.2",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.2.3"
  },
  "jest": {
    "testEnvironment": "node",
    "projects": [
      "<rootDir>/config/jest.server.config.js",
      "<rootDir>/config/jest.app.config.js"
    ]
  },
  "lint-staged": {
    "app/**/*.js": [
      "./node_modules/.bin/eslint --fix --ignore-path .eslintignore",
      "git add"
    ],
    "{config,scripts,server}/**/*.js": [
      "./node_modules/.bin/eslint --fix --ignore-path .eslintignore",
      "git add"
    ]
  }
}
